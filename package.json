{
  "name": "adopt-me",
  "version": "1.0.0",
  "description": "React V5",
  "main": "index.js",
  "scripts": {
    "build": "parcel build --public-url ./dist/ src/index.html",
    "dev:mock": "cross-env PET_MOCK=mock npm run dev",
    "format": "prettier --write \"./src/**/*.{js,html}\"",
    "lint": "eslint \"./src/**/*.{js,html}\" --quiet",
    "dev": "parcel src/index.html",
    "production": "parcel build src/index.html",
    "start": "npm -s run build && babel-node Server/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Alsaad",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "@babel/node": "^7.10.4",
    "@frontendmasters/pet": "^1.0.3",
    "@reach/router": "^1.3.4",
    "babel-cli": "^6.26.0",
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
